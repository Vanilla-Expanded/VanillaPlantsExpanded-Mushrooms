<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Plants Expanded - More Plants</li>
		</mods>
		<match Class="PatchOperationFindMod">
			<mods>
				<li>Ideology</li>
			</mods>
			<match Class="PatchOperationSequence">
				<operations>
					<li Class="PatchOperationAdd">
						<xpath>/Defs/ThingDef[defName="VCE_Moss"]/plant/sowTags</xpath>
						<value>
							<li>VPE_Mushroom</li>
						</value>
					</li>
					<li Class="PatchOperationAdd">
						<xpath>/Defs/ThingDef[defName="VCE_Tinctshroom"]/plant/sowTags</xpath>
						<value>
							<li>VPE_Mushroom</li>
						</value>
					</li>
					<li Class="PatchOperationAdd">
						<xpath>/Defs/ThingDef[defName="VCE_Healshroom"]/plant/sowTags</xpath>
						<value>
							<li>VPE_Mushroom</li>
						</value>
					</li>
				</operations>
			</match>
		</match>
	</Operation>
</Patch>

